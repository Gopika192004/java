import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Main {

    public static void main(String[] args) {
       Main obj=new Main();
       Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        obj.calculateSquare(n);
    }
    public void calculateSquare(int n){
        if (n>=1 && n<=9)
        {
            int square=n*n;
            System.out.println("Square of "+ n +" is "+ square);
        }
        else
        {
           System.out.println("Invalid Input");  
        }
    }
}
